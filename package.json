{
  "name": "frojo-website",
  "version": "1.0.0",
  "description": "Francisco Rojo's website",
  "repository": "https://github.com/frojo/personal-website",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "e2e": "repo-tools test deploy",
    "test": "repo-tools test app",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "author": "Francisco Rojo",
  "contributors": [
    {
      "name": "Francisco Rojo",
      "email": "mojoz0101@gmail.com"
    }
  ],
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "requiredEnvVars": [
          "GCLOUD_STORAGE_BUCKET"
        ],
        "args": [
          "app.js"
        ],
        "msg": "Francisco Rojo's website",
        "build": {
          "requiredEnvVars": [
            "GCLOUD_STORAGE_BUCKET"
          ]
        }
      }
    }
  },
  "license": "Apache-2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^1.6.0",
    "express": "4.16.2"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "2.2.1",
    "ava": "0.25.0",
    "supertest": "3.0.0"
  },
  "engines": {
    "node": "8.x.x"
  }
}
